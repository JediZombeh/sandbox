# TEMPLATING SNIPS

  'Comment Block':
    'prefix': 'comment'
    'body': """
    <!--#$1-->
    $2
    <!--/END$1-->
    """


  'Horizontal Rule [dark]':
    'prefix': 'hr-dark'
    'body': """
    <hr style="background-color:#404040;border:0;color:#404040;height:1px;width:90%;padding:0;" />
    """


  'Horizontal Rule [light]':
    'prefix': 'hr-dark'
    'body': """
    <hr style="background-color:#404040;border:0;color:#404040;height:1px;width:90%;padding:0;" />
    """


  'Horizontal Rule [spacer]':
    'prefix': 'spacer'
    'body': """
    <!--spacer--><hr style="height:16px; background-color:inherit;border:0;color:inherit;margin:0;width:90%;padding:0;" />
    """


  'One-Column Row':
    'prefix': 'temp-oneColumn'
    'body': """
    <tr><td class="one-column"><table width="100%"><tr><td class="inner contents" style="background-color: #FFFFFF;">
        [PLACE CONTENT HERE]
    </td></tr></table></td></tr><tr><td class="one-column"><table width="100%"><tr><td class="spacer">&nbsp;</td></tr></table></td></tr>
    """


  'Two-Column Row':
    'prefix': 'temp-twoColumn'
    'body': """
    <tr><td class="two-column" style="background-color: #FFFFFF;">
        <!--[if (gte mso 9)|(IE)]><table width="100%"><tr><td width="50%" valign="top"><![endif]-->
        <div class="column"><table width="100%"><tr><td class="inner"><table class="contents"><tr><td>
            [PLACE CONTENT HERE]
        </td></tr></table></td></tr></table></div>
        <!--[if (gte mso 9)|(IE)]></td><td width="50%" valign="top"><![endif]-->
        <div class="column"><table width="100%"><tr><td class="inner"><table class="contents"><tr><td>
            [PLACE CONTENT HERE]
        </td></tr></table></td></tr></table></div>
        <!--[if (gte mso 9)|(IE)]></td> </tr> </table><![endif]-->
    </td></tr><tr><td class="one-column"><table width="100%"><tr><td class="spacer">&nbsp;</td></tr></table></td></tr>
    """


  'Three-Column Row':
    'prefix': 'temp-threeColumn'
