  'Full-Width Banner':
    'prefix': 'temp-fullWidthBanner'
    'body': """
    <tr><td class="full-width-image">
        <a href="https://bannerLink.com" target="blank">
            <img src="http://via.placeholder.com/640x100?text=Full-Width+Banner" alt="full-width banner fpo text" width="640" height="auto">
        </a>
    </td></tr><tr><td class="one-column"><table width="100%"><tr><td class="spacer">&nbsp;</td></tr></table></td></tr>
    """


  'Banner':
